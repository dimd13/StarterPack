{
  "name": "agnostic-srp",
  "version": "0.0.3",
  "description": "Base files, folder structure, components based, SRP.",
  "scripts": {
    "start": "npm run dev:server",
    "dev:server": "node server",
    "rimraf": "./node_modules/.bin/rimraf",
    "build:prod": "rimraf build && webpack --progress --colors --config webpack.prod.config.js",
    "eslint": "./node_modules/.bin/eslint",
    "eslint:test": "eslint -c .eslintrc ./src/**/*.js || true",
    "eslint:format": "eslint:bin -c .eslintrc --fix ./src/**/*.js",
    "stylelint": "./node_modules/.bin/stylelint",
    "stylelint:test": "stylelint './src/**/*.css' --config .stylelintrc || true",
    "stylefmt": "./node_modules/.bin/stylefmt",
    "stylefmt:format": "stylefmt -c .stylelintrc -R ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethyde/agnostic-srp.git"
  },
  "author": "Ethyde",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/ethyde/agnostic-srp/issues"
  },
  "dependencies": {},
  "homepage": "https://github.com/ethyde/agnostic-srp#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.1.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.13.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylefmt": "^4.2.3",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "twig-loader": "^0.2.3",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
